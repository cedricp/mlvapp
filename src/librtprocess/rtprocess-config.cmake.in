@PACKAGE_INIT@

get_filename_component(RTPROCESS_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

if (EXISTS "${RTPROCESS_CMAKE_DIR}/CMakeCache.txt")
    # In build tree
    include(${RTPROCESS_CMAKE_DIR}/rtprocess-build-tree-settings.cmake)
else()
    set(RTPROCESS_INCLUDE_DIR @PACKAGE_INCLUDE_INSTALL_DIR@)
endif()

set(RTPROCESS_LIBRARY @PACKAGE_LIB_INSTALL_DIR@/@RTPROCESS_LIBRARY_NAME@)
set(RTPROCESS_LIBRARIES @PACKAGE_LIB_INSTALL_DIR@/@RTPROCESS_LIBRARY_NAME@)

mark_as_advanced(RTPROCESS_LIBRARY RTPROCESS_INCLUDE_DIR)
